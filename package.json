{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.3.0",
    "google-map-react": "^1.0.5",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "query-string": "5.1.1",
    "react": "^16.4.1",
    "react-calendar": "^2.16.1",
    "react-dom": "^16.4.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.4",
    "react-scroll": "^1.7.10",
    "react-scrolllock": "^3.0.1",
    "react-slick": "^0.21.0",
    "react-toastify": "^4.1.0",
    "slick-carousel": "^1.8.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --output-style compressed --include-path src/sass src/sass/main.sass src/main.css",
    "watch-css": "npm run build-css && node-sass-chokidar --output-style compressed --include-path src/sass src/sass/main.sass src/main.css -w -r",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "deploy": "npm run build && firebase deploy",
    "eject": "react-scripts eject"
  }
}

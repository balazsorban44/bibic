  "private": true,
  "dependencies": {
    "date-fns": "^2.0.0-alpha.26",
    "deep-equal": "^1.0.1",
    "firebase": "^5.7.1",
    "google-map-react": "^1.1.2",
    "react": "^16.7.0",
    "react-date-range": "balazsorban44/react-date-range#patch-1",
    "react-dom": "^16.7.0",
    "react-reveal": "^1.2.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "react-scrolllock": "^3.0.2",
    "react-toastify": "^4.5.1"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.0",
    "firebase-mock": "^2.2.10",
    "jest-fetch-mock": "^2.0.1",
    "node-sass": "^4.11.0",
    "react-scripts": "2.1.2",
    "source-map-explorer": "^1.6.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0"
  },
  "scripts": {
    "analyze": "source-map-explorer --html build/static/js/main.* > index.html",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "deploy": "npm run build && firebase deploy",
    "test": "CI=true react-scripts test",
    "test:watch": "react-scripts test --coverage --coverageReporters html --watch",
    "test:coverage": "react-scripts test --coverage --coverageReporters text-lcov | npx coveralls && rm -rf coverage"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*{js,jsx}",
      "!/node_modules/",
      "!src/lib/react-date-range/**/*{js,jsx}"
    ]
  }
}

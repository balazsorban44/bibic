{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.5.3",
    "google-map-react": "^1.0.9",
    "moment": "^2.22.2",
    "moment-range": "^4.0.1",
    "nuka-carousel": "^4.4.1",
    "query-string": "5.1.1",
    "react": "^16.5.2",
    "react-date-range": "^1.0.0-beta",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scroll": "^1.7.10",
    "react-scrolllock": "^3.0.2",
    "react-toastify": "^4.4.0"
  },
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "react-scripts": "2.0.4"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --output-style compressed --include-path src/sass src/sass/main.sass src/main.css",
    "watch-css": "npm run build-css && node-sass-chokidar --output-style compressed --include-path src/sass src/sass/main.sass src/main.css -w -r",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "deploy": "npm run build && firebase deploy",
    "eject": "react-scripts eject",
    "test": "react-scripts test"
}
